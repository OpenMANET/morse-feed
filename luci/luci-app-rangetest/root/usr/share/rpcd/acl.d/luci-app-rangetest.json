{
	"luci-app-rangetest": {
		"description": "Grant access to the LuCI range test app",
		"write": {
			"ubus": {
				"umdns": [ "update", "browse" ],
				"rangetest": [ "background_iperf3_server", "background_iperf3_client", "get_background", "iw_station_dump", "morse_cli_stats_reset", "morse_cli_stats" ],
				"rangetest-remote-rpc": [ "request" ]
			}
		},
		"read": {
			"file": {
				"/etc/init.d/umdns restart" : [ "exec" ]
			}
		}
	}
}